{
    "Imports" :{
        "DataTypes": "/service/test-service/test-data-types.json"
    },

    "id":"testsrv",
    "url":"http://localhost:4000/js/service/test-service/",
    "Resources": [
        { "id": "res1", "type":"Res", "key":"id", "methods": ["CREATE", "RETRIEVE", "UPDATE"] },
        { "id": "res2", "type":"Res", "key":"id", "methods": ["CREATE", "RETRIEVE", "UPDATE", "DELETE"] }
    ],
    "Endpoints": [
        { "id":"bind",
            "calls": [
                {   "method": "POST",
                    "request": {
                        "mimeType": "application/json",
                        "arguments": [
                            { "name":"res1", "type":"int" },
                            { "name":"res2", "type":"int" }
                        ]
                    },
                    "response": {
                        "mimeType": "application/json",
                        "type": {
                            "attributes": [
                                { "name":"id", "type":"int" },
                                { "name":"res1", "type":"Res" },
                                { "name":"res2", "type":"Res" }
                            ]
                        }
                    }
                }
            ]
        },
        { "id":"stats",
            "calls": [
                {   "method": "GET",
                    "request":{
                        "mimeType": "application/json",
                        "arguments": [
                            { "name":"id", "type":"int" }
                        ]
                    },
                    "response": {
                        "mimeType": "application/json",
                        "type": { "type":"map", "key":"int", "value":"Res" }
                    }
                }
            ]
        }
    ]
}
