{
    "style": {
        "left": 0, "top": 0,
        "width": "910px", "height": "59px",
        "z-index": 0,
        "background-color": "#206080",
        "background-image": "./ui/synth1-blank.png",
        "font": "consolas 10 normal",
        "border": "#204080 1px outset"
    },
    "elem-template": {
        "look": "knob",
        "data-type": "float",
        "decimal-digits": 2,
        "min": 0, "max": 2,
        "step": 0.001,
        "style": {
            "color": "#80c0ff",
            "background-color": "#204080",
            "background-image": "synth/ui/control-bg.png",
            "border": "#4060f0 1px outset",
            "font": "consolas 9 normal",
            "width": "24px", "height": "12px",
            "align": "center middle"
        }
    },
    "group-template": {
        "style": {
            "border": "#4060a0 0px outset",
            "padding": "1px"
        }
    },
    "groups": {
        "env": {
            "style": {
                "background-image": "",
                "width": "138px", "height": "59px"
            },
            "controls": {
                "amp": { "type": "Textbox", "style": { "left":   "2px", "top": "20px" } },
                "atk": { "type": "Textbox", "style": { "left":  "29px", "top": "20px" } },
                "dec": { "type": "Textbox", "style": { "left":  "56px", "top": "20px" } },
                "sus": { "type": "Textbox", "style": { "left":  "83px", "top": "20px" } },
                "rel": { "type": "Textbox", "style": { "left": "110px", "top": "20px" } }
            }
        },
        "lfo": {
            "style": {
                "background-image": "",
                "width": "55px", "height": "59px"
            },
            "controls": {
                "amp": { "type": "Textbox", "style": { "left":  "2px", "top":  "20px"}, "decimal-digits": 2 },
                "fre": { "type": "Textbox", "style": { "left":  "29px", "top": "20px"}, "decimal-digits": 1 }
            }
        },
        "osc": {
            "style": {
                "background-image": "",
                "width": "137px", "height": "59px"
            },
            "controls": {
                "amp":  { "type": "Textbox", "style": { "left":   "2px", "top": "20px" } },
                "fre":  { "type": "Textbox", "style": { "left":  "29px", "top": "20px" }, "decimal-digits": 0 },
                "psw":  { "type": "Textbox", "style": { "left":  "56px", "top": "20px" } },
                "tune": { "type": "Textbox", "style": { "left":  "83px", "top": "20px" }, "data-type": "int", "decimal-digits": 0, "min": 0, "max": 24, "step": 0.5 },
                "wave": { "type": "Textbox", "style": { "left": "110px", "top": "20px" }, "data-type": "int", "decimal-digits": 0, "step": 1 }
            }
        },
        "flt": {
            "style": {
                "background-image": "",
                "width": "82px", "height": "59px"
            },
            "controls": {
                "mode": { "type": "Textbox", "style": { "left": "2px", "top": "20px" }, "data-type": "int", "decimal-digits": 0 },
                "cut": { "type": "Textbox", "style": { "left": "29px", "top": "20px" } },
                "res": { "type": "Textbox", "style": { "left": "56px", "top": "20px" } }}
        }
    },
    "layout": {
        "poly": { "type": "Textbox", "style": { "left":   "2px", "top": "21px" }, "data-type": "int", "decimal-digits": 0, "min": 1, "max": 16, "step": 1 },
        "env1": { "group": "env",    "style": { "left":  "84px", "top":  "0px" } },
        "env2": { "group": "env",    "style": { "left": "277px", "top":  "0px" } },
        "env3": { "group": "env",    "style": { "left": "771px", "top":  "0px" } },
        "lfo1": { "group": "lfo",    "style": { "left":  "28px", "top":  "0px" } },
        "lfo2": { "group": "lfo",    "style": { "left": "221px", "top":  "0px" } },
        "osc1": { "group": "osc",    "style": { "left": "414px", "top":  "0px" } },
        "osc2": { "group": "osc",    "style": { "left": "551px", "top":  "0px" } },
        "flt1": { "group": "flt",    "style": { "left": "688px", "top":  "0px" } }
    }
}