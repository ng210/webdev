{
    "template": {
        "images": {
            "type":"ddlist", "data-field": "images", "label":true,
            "events": ["change"]
        },
        "zoom": {
            "type":"slider", "data-field": "zoom",
            "data-type":"float", "min": -2.0, "max": 2.0, "step": 0.1
        },
        "rotation": {
            "type":"slider", "data-field": "rotation",
            "data-type":"float", "min": -1.0, "max": 1.0, "step": 0.1
        },
        "time": {
            "type":"slider", "data-field": "time",
            "data-type":"float", "min": 0.0, "max": 0.5, "step": 0.01
        },
        "interpolation": {
            "type":"ddlist", "data-field": "interpolation", "label":true,
            "items": ["none", "h-linear", "v-linear", "bilinear"],
            "events": ["change"]
        }
    },
    "settings": {
        "zoom": 1.0,
        "rotation": 0.25,
        "time": 0.1,
        "interpolation": "none",
        "images": 1
    }
}