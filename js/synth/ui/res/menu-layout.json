{
    "id": "Menu",
    "type": "Menu",
    "layout": "horizontal",
    "style": {
        "font": "Arial 18px",
        "align":"center middle",
        "border":"#607080 1px outset",
        "color": "#001010",
        "background-color": "#607080",
        "spacing": "2px 1px",
        "padding": "0px 0px",
        "z-index": 100
    },
    "item-template": {
        "type": "Label",
        "layout": "vertical",
        "style": {
            "border":"#607080 1px outset",
            "align": "left middle",
            "padding": "3px 2px",
            "width": "10em",
            "height": "1.2em"
        }
    }
}